{
    "manifest_version": 3,
    "name": "Wordle Extension Game",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
      "default_popup": "hello.html",
    "permissions": ["scripting", "windows", "storage", "activeTab"]
    },
    "web-accessible_resources":[
      {
        "resources": ["WordleList.txt"],
        "matches": ["<all_urls>"]
      }
    ],
    "background":{
      "service_worker": "background.js"
    }

  }